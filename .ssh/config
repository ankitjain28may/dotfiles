# Personal account, - the default config
Host github.com
   HostName github.com
   User git
   IdentityFile ~/.ssh/id_personal

# Work account
Host github-work.com
   HostName github-work.com
   User git
   IdentityFile ~/.ssh/id_work

Host <alias1>
   HostName <IP_ADDRESS>
   ForwardAgent Yes
   User <user_name>
   IdentityFile <path_of_pem_file>

Host <alias2>
   Hostname <IP_ADDRESS>
   ForwardAgent Yes
   User <user_name>
   TcpKeepAlive yes